import java.util.*;
public class largestno{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        
        if(a==c || b==c || b==a){
            if(a == c){
                System.out.println("A and C are equal");
            }
            else if(b == c ){
                System.out.println("B and C are equal");
            }
            else {
                System.out.println("A and B are equal");
            }
        }
        else if(a>=b && a>=c ){
            System.out.println("A is largestno");
        }
        else if (b>=c && b>=a){
            System.out.println("B is largestno");
        }
        else {
            System.out.println("C is largestno");
        }
        
    }
}
